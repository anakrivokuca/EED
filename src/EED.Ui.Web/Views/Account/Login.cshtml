@model EED.Ui.Web.Models.User.LoginViewModel

@{
    ViewBag.Title = "Login";
}

<hgroup class="title">
    <h1>@ViewBag.Title</h1>
</hgroup>

@using(Html.BeginForm()) {
    @Html.ValidationSummary(true)
    @Html.EditorForModel()
    <p><input type="submit" value="Log in" /></p>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}